pipeline{
      agent any
      stages{
      stage('compile stage'){
        steps{
        withmaven(maven : 'maven_3_3_9'){
        sh'mvn clean compile'
        }
      }
    }
    stage('test stage'){
    steps{
    withmaven(maven : 'maven_3_3_9'){
    sh'mvn test'
    }
   }
}
    stage('deployment stage'){
    steps{
    withmaven(maven : 'maven_3_3_9'){
    sh'mvn deploy
     }
   }
 }
 }
 }
